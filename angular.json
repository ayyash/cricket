{
   "version": 1,
   "newProjectRoot": "projects",
   "projects": {
      "cricketweb": {
         "root": "",
         "sourceRoot": "src",
         "schematics": {
            "@schematics/angular:component": {
               "style": "less"
            },
            "@schematics/angular:application": {
               "strict": true
            }
         },
         "projectType": "application",
         "prefix": "cr",
         "architect": {
            "writeindex": {
               "builder": "./builder:localizeIndex",
               "options": {
                  "source": "host/client/placeholder.html",
                  "destination": "host/index",
                  "isUrlBased": true,
                  "languages": [
                     {
                        "name": "ar",
                        "isRtl": true
                     },
                     {
                        "name": "en",
                        "isRtl": false
                     },
                     {
                        "name": "fr",
                        "isRtl": false
                     }
                  ]
               }
            },
            "extract": {
               "builder": "./builder:extract",
               "options": {
                  "destination": "./src/locale",
                  "scan": "./src/app/components",
                  "languages": [
                     {
                        "name": "en",
                        "localeId": "en"
                     },
                     {
                        "name": "ar",
                        "localeId": "ar-JO",
                        "isDefault": true
                     },
                     {
                        "name": "fr",
                        "localeId": "fr-CA"
                     }
                  ]
               }
            },
            "build": {
               "builder": "@angular-devkit/build-angular:browser",
               "options": {
                  "outputPath": "dist",
                  "resourcesOutputPath": "assets/",
                  "allowedCommonJsDependencies": [
                     "express"
                  ],
                  "index": "src/placeholder.html",
                  "main": "src/main.ts",
                  "polyfills": ["zone.js"],
                  "tsConfig": "tsconfig.app.json",
                  "styles": [
                     {
                        "input": "src/assets/css/cricket.critical.css",
                        "bundleName": "critical.ltr",
                        "inject": false
                     },
                     {
                        "input": "src/assets/css/cricket.critical.rtl.css",
                        "bundleName": "critical.rtl",
                        "inject": false
                     },
                     {
                        "input": "src/assets/css/cricket.general.css",
                        "bundleName": "styles.ltr",
                        "inject": false
                     },
                     {
                        "input": "src/assets/css/cricket.general.rtl.css",
                        "bundleName": "styles.rtl",
                        "inject": false
                     }
                  ],
                  "stylePreprocessorOptions": {
                     "includePaths": [
                        "./raw/less",
                        "./node_modules/shut/src/less"
                     ]
                  },
                  "scripts": [
                     {
                        "input": "src/assets/js/cricket.js",
                        "bundleName": "cricket"
                     }
                  ],
                  "assets": [
                     {
                        "glob": "favicon.ico",
                        "input": "src",
                        "output": "/"
                     },
                     {
                        "glob": "images/**/*",
                        "input": "src/assets",
                        "output": "/assets"
                     },
                     {
                        "glob": "*.js",
                        "input": "src/locale",
                        "output": "/locale"
                     },
                     {
                        "glob": "*.json",
                        "input": "configs",
                        "output": "/localdata"
                     },
                     {
                        "glob": "*.json",
                        "input": "localdata",
                        "output": "/localdata"
                     },
                     {
                        "glob": "*(ar-JO|en).js",
                        "input": "node_modules/@angular/common/locales/global",
                        "output": "/locale"
                     }
                  ]
               },
               "configurations": {
                  "serve": {
                     "buildOptimizer": false,
                     "optimization": false,
                     "vendorChunk": true,
                     "extractLicenses": false,
                     "sourceMap": true,
                     "namedChunks": true,
                     "commonChunk": true,
                     "aot": true,
                     "index": "src/index.dev.html",
                     "styles": [
                        {
                           "input": "src/assets/css/cricket.css",
                           "bundleName": "styles"
                        }
                     ]
                  },
                  "serveRtl": {
                     "buildOptimizer": false,
                     "optimization": false,
                     "vendorChunk": true,
                     "extractLicenses": false,
                     "sourceMap": true,
                     "namedChunks": true,
                     "commonChunk": true,
                     "aot": true,
                     "index": "src/index.rtl.html",
                     "styles": [
                        {
                           "input": "src/assets/css/cricket.rtl.css",
                           "bundleName": "styles"
                        }
                     ]
                  },
                  "production": {
                     "outputPath": "./host/client/",
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.prod.ts"
                        }
                     ],
                     "optimization": {
                        "scripts": true,
                        "fonts": false,
                        "styles": true
                     },
                     "outputHashing": "all",
                     "sourceMap": false,
                     "namedChunks": false,
                     "aot": true,
                     "extractLicenses": true,
                     "vendorChunk": false,
                     "buildOptimizer": true,
                     "deleteOutputPath": true,
                     "assets": [
                        {
                           "glob": "favicon.ico",
                           "input": "src",
                           "output": "/"
                        },
                        {
                           "glob": "images/**/*",
                           "input": "src/assets",
                           "output": "/assets"
                        },
                        {
                           "glob": "*(cr-en|cr-ar).js",
                           "input": "src/locale",
                           "output": "/locale"
                         },
                         {
                           "glob": "*(ar-JO|en).js",
                           "input": "node_modules/@angular/common/locales/global",
                           "output": "/locale"
                         }
                     ],
                     "budgets": [
                        {
                           "type": "initial",
                           "maximumWarning": "500kb",
                           "maximumError": "5mb"
                        },
                        {
                           "type": "anyComponentStyle",
                           "maximumWarning": "2kb",
                           "maximumError": "4mb"
                        }
                     ]
                  }
               }
            },
            "serve": {
               "builder": "@angular-devkit/build-angular:dev-server",
               "options": {
                  "browserTarget": "cricketweb:build:serve"
               },
               "configurations": {
                  "production": {
                     "browserTarget": "cricketweb:build:serveProduction"
                  },
                  "rtl": {
                     "browserTarget": "cricketweb:build:serveRtl"
                  }
               }
            },
            "server": {
               "builder": "@angular-devkit/build-angular:server",
               "options": {
                  "outputPath": "./host/server/ng",
                  "main": "server.ts",
                  "tsConfig": "tsconfig.server.json",
                  "stylePreprocessorOptions": {
                     "includePaths": [
                        "./raw/less",
                        "./node_modules/shut/src/less"
                     ]
                  }
               },
               "configurations": {
                  "production": {
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.prod.ts"
                        }
                     ],
                     "optimization": false,
                     "outputHashing": "media",
                     "sourceMap": false,
                     "namedChunks": true,
                     "extractLicenses": true,
                     "deleteOutputPath": false
                  }
               }
            },
            "prerender": {
               "builder": "./prerender-builder:single",
               "options": {
                 "browserTarget": "cricketweb:build:production",
                 "serverTarget": "cricketweb:server:production",
                 "routes": [
                   "/routes/here"
                 ]
               }
             },
             "prerender-multi": {
               "builder": "./prerender-builder:multi",
               "options": {
                 "browserTarget": "cricketweb:build:production",
                 "serverTarget": "cricketweb:server:production",
                 "indexTarget": "cricketweb:writeindex",
                 "localePath": "locale",
                 "routes": [
                   "/routes/here"
                 ]
               }
             }
         }
      }
   }
}
