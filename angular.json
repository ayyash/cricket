{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "defaultProject": "cricketweb",
    "projects": {
        "cricketweb": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "cr",
            "architect": {
                "build": {
                    "browserslist": true,
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/en",
                        "resourcesOutputPath": "assets/",
                        "baseHref": "/en/",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "cricket_favicon.png",
                                "input": "src",
                                "output": "/"
                            },
                            {
                                "glob": "images/**/*",
                                "input": "src/assets",
                                "output": "/assets"
                            }
                        ],
                        "styles": [
                            {
                                "input": "src/assets/css/cr.critical.css",
                                "bundleName": "critical"
                            },
                            {
                                "input": "src/assets/css/cr.general.css",
                                "bundleName": "styles",
                                "lazy": true
                            }
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "./mockup/less",
                                "./node_modules/shut/src/less"
                            ]
                        },
                        "scripts": [
                            {
                                "input": "src/assets/js/cricket.js",
                                "bundleName": "cricket"
                            }
                        ]
                    },
                    "configurations": {
                       
                        "serve": {
                            "extractCss": true,
                            "namedChunks": true,
                            "vendorChunk": false,
                            "commonChunk": true,
                            "optimization": false,
                            "aot": true,
                            "showCircularDependencies": false,
                            "sourceMap": true,
                            "index": "src/index.dev.html",
                            "styles": [
                                {
                                    "input": "src/assets/css/cr.css",
                                    "bundleName": "styles"
                                }
                            ],
                            "assets": [
                                {
                                    "glob": "cricket_favicon.png",
                                    "input": "src",
                                    "output": "/"
                                },
                                {
                                    "glob": "images/**/*",
                                    "input": "src/assets",
                                    "output": "/assets"
                                },
                                {
                                    "glob": "*.json",
                                    "input": "src/localdata",
                                    "output": "/localdata"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "configs",
                                    "output": "/localdata"
                                }
                            ]
                        },
                        "network": {
                            "extractCss": true,
                            "namedChunks": true,
                            "vendorChunk": false,
                            "commonChunk": true,
                            "aot": true,
                            "showCircularDependencies": false,
                            "sourceMap": true,
                            "index": "src/index.dev.html",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.network.ts"
                                },
                                {
                                    "replace": "configs/config.json",
                                    "with": "configs/config.network.json"
                                }
                            ],
                            "styles": [
                                {
                                    "input": "src/assets/css/cr.css",
                                    "bundleName": "styles"
                                }
                            ]
                        },
                        "ar": {
                            "extractCss": true,
                            "namedChunks": true,
                            "vendorChunk": false,
                            "commonChunk": true,
                            "optimization": false,
                            "aot": true,
                            "showCircularDependencies": false,
                            "sourceMap": true,
                            "index": "src/index.dev.html",
                            "styles": [
                                {
                                    "input": "src/assets/css/cr.rtl.css",
                                    "bundleName": "styles"
                                }
                            
                            ],
                            "assets": [
                                {
                                    "glob": "cricket_favicon.png",
                                    "input": "src",
                                    "output": "/"
                                },
                                {
                                    "glob": "images/**/*",
                                    "input": "src/assets",
                                    "output": "/assets"
                                },
                                {
                                    "glob": "*.json",
                                    "input": "src/localdata",
                                    "output": "/localdata"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "configs",
                                    "output": "/localdata"
                                }
                            ],

                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.ar.ts"
                                },
                                {
                                    "replace": "src/locale/resources.ts",
                                    "with": "src/locale/resources.ar.ts"
                                }
                            ],
                            "outputPath": "dist/ar",
                            "baseHref": "/ar/"
                        },
                        "serveProduction": {
                            "extractCss": true,
                            "namedChunks": true,
                            "vendorChunk": false,
                            "commonChunk": true,
                            "optimization": false,
                            "aot": true,
                            "showCircularDependencies": false,
                            "sourceMap": true,
                            "index": "src/index.dev.html",
                            "fileReplacements": [
                                {
                                    "replace": "configs/config.json",
                                    "with": "configs/config.prod.json"
                                }],
                            "styles": [
                                {
                                    "input": "src/assets/css/cr.css",
                                    "bundleName": "styles"
                                
                                }
                            ],
                            "assets": [
                                {
                                    "glob": "cricket_favicon.png",
                                    "input": "src",
                                    "output": "/"
                                },
                                {
                                    "glob": "images/**/*",
                                    "input": "src/assets",
                                    "output": "/assets"
                                },
                                {
                                    "glob": "*.json",
                                    "input": "src/localdata",
                                    "output": "/localdata"
                                },
                                {
                                    "glob": "config.json",
                                    "input": "configs",
                                    "output": "/localdata"
                                }
                            ]
                        },
                        "production": {
                            "outputPath": "../cricket.host/en",
                            "baseHref": "/en/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "deleteOutputPath": true,
                            "showCircularDependencies": false
                        },
                        "productionAr": {
                            "outputPath": "../cricket.host/ar",
                            "baseHref": "/ar/",
                            "index": "src/index.rtl.html",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ar.ts"
                                },
                                {
                                    "replace": "src/locale/resources.ts",
                                    "with": "src/locale/resources.ar.ts"
                                }
                            ],
                            "styles": [
                                {
                                    "input": "src/assets/css/cr.critical.rtl.css",
                                    "bundleName": "critical-rtl"
                                },
                                {
                                    "input": "src/assets/css/cr.general.rtl.css",
                                    "bundleName": "styles-rtl",
                                    "lazy": true
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "deleteOutputPath": true,
                            "showCircularDependencies": false
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "cricketweb:build:serve"
                    },
                    "configurations": {
                        "network": {
                            "browserTarget": "cricketweb:build:network"
                        },
                        "ar": {
                            "browserTarget": "cricketweb:build:ar"
                        },
                        "production": {
                            "browserTarget": "cricketweb:build:serveProduction"
                        }
                    }
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "outputPath": "dist/en",
                        "main": "src/main.server.ts",
                        "tsConfig": "src/tsconfig.server.json",
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "./mockup/less",
                                "./node_modules/shut/src/less"
                            ]
                        }
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "media",
                            "sourceMap": false,
                            "namedChunks": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "deleteOutputPath": true,
                            "showCircularDependencies": false
                        },
                        "productionAr": {
                            "outputPath": "dist/ar",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ar.ts"
                                },
                                {
                                    "replace": "src/locale/resources.ts",
                                    "with": "src/locale/resources.ar.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "media",
                            "sourceMap": false,
                            "namedChunks": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "deleteOutputPath": true,
                            "showCircularDependencies": false
                        }
                    }
                }
            }
        }
    }
}
